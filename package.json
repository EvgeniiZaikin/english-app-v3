{
  "name": "english-app-v3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "npm run build && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "npx tsm src --nameFormat none",
    "create-db-migration": "cd ./src/database && node ../../node_modules/db-migrate/bin/db-migrate create $NAME",
    "apply-db-migration": "cd ./src/database && node ../../node_modules/db-migrate/bin/db-migrate up",
    "cancel-db-migration": "cd ./src/database && node ../../node_modules/db-migrate/bin/db-migrate down",
    "reset-db-migrations": "cd ./src/database && node ../../node_modules/db-migrate/bin/db-migrate reset"
  },
